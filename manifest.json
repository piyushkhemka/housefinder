{
    "name": "Streeteasy helper",
    "version": "1.0",
    "manifest_version": 3,
    "description": "Helps you find additional information about houses listed on Streeteasy",
    "icons": {
        "128": "icon128.png"
    },
    "content_scripts": [
        {
          "matches": ["https://*.streeteasy.com/*"],
          "js": ["background.js"]
        }
      ],
    "action": {
        "default_popup": "popup.html"
    }
}